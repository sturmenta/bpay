{
  "expo": {
    "jsEngine": "hermes",
    "name": "bpay",
    "slug": "bpay",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./src/assets/app_images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/app_images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.sturmenta.bpay"
    },
    "android": {
      "package": "com.sturmenta.bpay",
      "versionCode": 1,
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/app_images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/app_images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c449748b-de14-476b-8ead-4f0efb1aa5f4"
      }
    }
  }
}
